43e\u043b\u043e\u0441\u0442\u0432\u043e\u0432\u044b\u0445. \u041d\u0430 \u043e\u0442\u043a\u0440\u044b\u0442\u043e\u0439 \u0432\u0435\u0440\u0430\u043d\u0434\u0435 \u0438\u0441\u043f\u0440\u043e\u0431\u043e\u0432\u0430\u0442\u044c \u0022\u0410\u0432\u0434\u043e\u0442\u044c\u0438\u043d \u0447\u0430\u0439\u0022. \u041d\u0430\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u043e\u0435 \u0443\u0433\u043e\u0449\u0435\u043d\u0438\u0435: \u043f\u0438\u0440\u043e\u0433 \u0438 \u043d\u0430\u043b\u0438\u0432\u043a\u0430. \u041f\u0440\u043e\u0434\u043e\u043b\u0436\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u044c 4 \u0447\u0430\u0441\u0430.\n\u0022\u0415\u0441\u0442\u044c \u0442\u0430\u043a\u043e\u0439 \u0433\u043e\u0440\u043e\u0434 \u0422\u0435\u0442\u044e\u0448\u0438 \u043d\u0430 \u0434\u0440\u0435\u0432\u043d\u0435\u0439 \u0411\u0443\u043b\u0433\u0430\u0440\u0441\u043a\u043e\u0439 \u0437\u0435\u043c\u043b\u0435. \u041a\u0430\u043a \u0443 \u0412\u043e\u043b\u0433\u0438 \u0443 \u0440\u0435\u043a\u0438 \u0436\u0438\u043b\u0438-\u0431\u044b\u043b\u0438 \u0440\u044b\u0431\u0430\u043a\u0438\u0022 \u0441 \u043d\u0430\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u044b\u043c \u0443\u0433\u043e\u0449\u0435\u043d\u0438\u0435\u043c\u041f\u0440\u0438\u0441\u0442\u0430\u043d\u044c-\u0422\u0435\u0442\u044e\u0448\u0438-\u041f\u0440\u0438\u0441\u0442\u0430\u043d\u044c. \u0410\u0432\u0442\u043e\u0431\u0443\u0441\u043d\u043e-\u043f\u0435\u0448\u0435\u0445\u043e\u0434\u043d\u0430\u044f \u044d\u043a\u0441\u043a\u0443\u0440\u0441\u0438\u044f \u043f\u043e \u0441\u0442\u0430\u0440\u0438\u043d\u043d\u043e\u043c\u0443 \u043a\u0443\u043f\u0435\u0447\u0435\u0441\u043a\u043e\u043c\u0443 \u0446\u0435\u043d\u0442\u0440\u0443 \u0433\u043e\u0440\u043e\u0434\u0430. \u0412\u0441\u0442\u0440\u0435\u0447\u0430 \u043d\u0430 \u043f\u0440\u0438\u0447\u0430\u043b\u0435. \u041f\u0440\u0438\u0432\u0435\u0442\u0441\u0442\u0432\u0438\u0435 \u043d\u0430\u0440\u043e\u0434\u043d\u043e\u0433\u043e \u0430\u043