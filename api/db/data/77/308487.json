3 \u0447\u0430\u0441\u0430.\n\u041e\u0441\u043d\u043e\u0432\u043d\u0430\u044f \u044d\u043a\u0441\u043a\u0443\u0440\u0441\u0438\u044f \u0433\u0440\u0443\u043f\u043f\u0430 \u21162:&nbsp;\u041f\u0435\u0440\u0435\u0435\u0437\u0434 \u043d\u0430 \u0430\u0432\u0442\u043e\u0431\u0443\u0441\u0435 \u0421\u043e\u0441\u043d\u043e\u0432\u0435\u0446 - \u0411\u0435\u043b\u043e\u043c\u043e\u0440\u0441\u043a. \u041f\u0435\u0440\u0435\u0435\u0437\u0434 \u043d\u0430 \u0442\u0435\u043f\u043b\u043e\u0445\u043e\u0434\u0435 &laquo;\u0421\u0430\u043f\u0444\u0438\u0440&raquo; \u0411\u0435\u043b\u043e\u043c\u043e\u0440\u0441\u043a - \u0421\u043e\u043b\u043e\u0432\u0435\u0446\u043a\u0438\u0435 \u043e-\u0432\u0430. \u0412\u0440\u0435\u043c\u044f \u0432 \u043f\u0443\u0442\u0438 \u043d\u0430 \u0442\u0435\u043f\u043b\u043e\u0445\u043e\u0434\u0435 4 \u0447\u0430\u0441\u0430.\u041e\u0431\u0437\u043e\u0440\u043d\u0430\u044f \u043f\u0435\u0448\u0435\u0445\u043e\u0434\u043d\u0430\u044f \u044d\u043a\u0441\u043a\u0443\u0440\u0441\u0438\u044f \u043f\u043e \u0421\u043e\u043b\u043e\u0432\u0435\u0446\u043a\u043e\u043c\u0443 \u043c\u043e\u043d\u0430\u0441\u0442\u044b\u0440\u044e. \u042d\u043a\u0441\u043a\u0443\u0440\u0441\u0438\u044f \u0437\u043d\u0430\u043a\u043e\u043c\u0438\u0442 \u0432\u0430\u0441 \u0441 \u0438\u0441\u0442\u043e\u0440\u0438\u0435\u0439 \u0421\u043e\u043b\u043e\u0432\u0435\u0446\u043a\u043e\u0433\u043e \u043c\u043e\u043d\u0430\u0441\u0442\u044b\u0440\u044f. \u0412\u044b \u043f\u043e\u0441\u0435\u0442\u0438\u0442\u0435 \u0442\u0435\u0440\u0440\u0438\u0442\u043e\u0440\u0438\u044e \u0446\u0435\u043d\u0442\u0440\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u043a\u043e\u043c\u043f\u043b\u0435\u043a\u0441\u0430 \u043c\u043e\u043d\u0430\u0441\u0442\u044b\u0440\u044f, \u0434\u0435\u0439\u0441\u0442\u0432\u0443\u044e\u0449\u0438\u0435 \u0445\u0440\u0430\u043c\u044b, \u043f\u0430\u043c\u044f\u0442\u043d\u0438\u043a\u0438, \u0432\u043a\u043b\u044e\u0447\u0435\u043d\u043d\u044b\u0435 \u0432 \u0421\u043f\u0438\u0441\u043e\u043a \u0432\u0441\u0435\u043c\u0438\u0440\u043d\u043e\u0433\u043e \u043d\u0430\u0441\u043b\u0435\u0434\u0438\u044f \u042e\u041d\u0415\u0421\u041a\u041e, \u043e\u0431\u044a\u0435\u043a\u0442\u044b \u0445\u043e\u0437\u044f\u0439\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0439 \u0434\u0435\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u0438 \u043c\u043e\u043d\u0430\u0441\u0442\u044b\u0440\u044f,\u0441\u043e\u043b\u043e\u0432\u0435\u0446\u043a\u0443\u044e \u043a\u0440\u0435\u043f\u043e\u0441\u0442\u044c (3 \u0447\u0430\u0441\u0430), \u0434\u0430\u043b\u0435\u0435 \u0441\u0432\u043e\u0431\u043e\u0434\u043d\u043e\u0435 \u0432\u0440\u0435\u043c\u044f.\n\u0414\u043e\u043f\u043e\u043b\u043d\u0438\u0442\u0435\u043b\u044c\u043d\u0430\u044f \u044d\u043a\u0441\u043a\u0443\u0440\u0441\u0438\u044f:&laquo;\u0411\u043e\u0442\u0430\u043d\u0438\u0447\u0435\u0441\u043a\u0438\u0439 \u0441\u0430\u0434 &ndash; \u041c\u0430\u043a\u0430\u0440\u044c\u0435\u0432\u0441\u043a\u0430\u044f \u043f\u0443\u0441\u0442\u044b\u043d\u044c&raquo;&nbsp;\u0430\u0432\u0442\u043e\u0431\u0443\u0441\u043d\u0430\u044f \u044d\u043a\u0441\u043a\u0443\u0440\u0441\u0438\u044f. \u0412\u044b \u043f\u043e\u0441\u0435\u0442\u0438\u0442\u0435 \u043e\u0434\u0438\u043d \u0438\u0437 \u0441\u0430\u043c\u044b\u0445 \u0441\u0435\u0432\u0435\u0440\u043d\u044b\u0445 \u0431\u043e\u0442\u0430\u043d\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u0441\u0430\u0434\u043e\u0432 \u0420\u043e\u0441\u0441\u0438\u0438, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0440\u0430\u0441\u043f\u043e\u043b\u043e\u0436\u0435\u043d \u043d\u0430 \u0442\u0435\u0440\u0440\u0438\u0442\u043e\u0440\u0438\u0438 \u0431\u044b\u0432\u0448\u0435\u0439 \u041c\u0430\u043a\u0430\u0440\u044c\u0435\u0432\u0441\u043a\u043e\u0439 \u043f\u0443\u0441\u0442\u044b\u043d\u0438, \u0443\u0432\u0438\u0434\u0438\u0442\u0435 \u043f\u0430\u043c\u044f\u0442\u043d\u0438\u043a\u0438, \u0443\u043d\u0438\u043a\u0430\u043b\u044c\u043d\u044b\u0435 \u043f\u043e\u0441\u0430\u0434\u043a\u0438 \u0440\u0430\u0437\u043b\u0438\u0447\u043d\u044b\u0445 \u043f\u0435\u0440\u0438\u043e\u0434\u043e\u0432 \u0438\u0441\u0442\u043e\u0440\u0438\u0438 \u0421\u043e\u043b\u043e\u0432\u0435\u0446\u043a\u0438\u0445 \u043e\u0441\u0442\u0440\u043e\u0432\u043e\u0432, \u043d\u0430\u0441\u043b\u0430\u0434\u0438\u0442\u0435\u0441\u044c \u0443\u0434\u0438\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u043c \u0432\u0438\u0434\u043e\u043c \u043d\u0430 \u0430\u043d\u0441\u0430\u043c\u0431\u043b\u044c \u0421\u043e\u043b\u043e\u0432\u0435\u0446\u043a\u043e\u0433\u043e \u043c\u043e\u043d\u0430\u0441\u0442\u044b\u0440\u044f, \u043e\u0442\u043a\u0440\u044b\u0432\u0430\u044e\u0449\u0438\u0439\u0441\u044f \u0441 \u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440\u043e\u0432\u0441\u043a\u043e\u0439 \u0433\u043e\u0440\u043a\u0438 (1,30 \u0447\u0430\u0441\u0430).\n\u041f\u0440\u0438\u043c\u0435\u0447\u0430\u043d\u0438\u0435:&nbsp;\u0440\u0430\u0441\u0441\u0442\u043e\u044f\u043d\u0438\u0435 \u043e\u0442 \u043f\u043e\u0441\u0451\u043b\u043a\u0430 \u0434\u043e \u0411\u043e\u0442\u0430\u043d\u0438\u0447\u0435\u0441\u043a\u043e\u0433\u043e \u0441\u0430\u0434\u0430 - 4 \u043a\u043c. \u0412 \u0441\u043b\u0443\u0447\u0430\u0435 \u0437\u0430\u043a\u0440\u044b\u0442\u0438\u044f \u0434\u043e\u0440\u043e\u0433\u0438 &laquo;\u043f. \u0421\u043e\u043b\u043e\u0432\u0435\u0446\u043a\u0438\u0439 - \u0433. \u0421\u0435\u043a\u0438\u0440\u043d\u0430\u044f&raquo; \u043e\u0440\u0433\u0430\u043d\u0430\u043c\u0438 \u0413\u0418\u0411\u0414\u0414 \u0430\u0432\u0442\u043e\u0431\u0443\u0441\u043d\u0430\u044f \u044d\u043a\u0441\u043a\u0443\u0440\u0441\u0438\u044f \u0431\u0443\u0434\u0435\u0442 \u043e\u0442\u043c\u0435\u043d\u0435\u043d\u0430.\n\u041f\u043e\u043b\u0435\u0437\u043d\u044b\u0435 \u0441\u043e\u0432\u0435\u0442\u044b:-&nbsp;\u041d\u0430&nbsp;\u0421\u043e\u043b\u043e\u0432\u0435\u0446\u043a\u0438\u0445 \u043e-\u0432\u0430\u0445&nbsp;\u043d\u0435\u0442 \u0431\u0430\u043d\u04