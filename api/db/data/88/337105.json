0438\u0445\u0430.\u00a0\u0412\u043e \u0432\u0440\u0435\u043c\u044f \u044d\u043a\u0441\u043a\u0443\u0440\u0441\u0438\u0438 \u0412\u044b \u0441\u043e\u0432\u0435\u0440\u0448\u0438\u0442\u0435 \u043f\u0443\u0442\u0435\u0448\u0435\u0441\u0442\u0432\u0438\u0435 \u043f\u043e \u0434\u0432\u043e\u0440\u044f\u043d\u0441\u043a\u043e\u0439 \u0443\u0441\u0430\u0434\u044c\u0431\u0435, \u0443\u0437\u043d\u0430\u0435\u0442\u0435 \u043e \u0435\u0435 \u0438\u0441\u0442\u043e\u0440\u0438\u0438 \u0438 \u0432\u043b\u0430\u0434\u0435\u043b\u044c\u0446\u0430\u0445. \u041f\u0435\u0440\u0435\u043d\u0435\u0441\u0435\u0442\u0435\u0441\u044c \u0432 \u043f\u0440\u043e\u0448\u043b\u043e\u0435 \u0438 \u043e\u043a\u0443\u043d\u0435\u0442\u0435\u0441\u044c \u0432 \u0430\u0442\u043c\u043e\u0441\u0444\u0435\u0440\u0443 \u0440\u0430\u0437\u043c\u0435\u0440\u0435\u043d\u043d\u043e\u0439 \u0443\u0441\u0430\u0434\u0435\u0431\u043d\u043e\u0439 \u0436\u0438\u0437\u043d\u0438, \u0433\u0434\u0435 \u0432\u0441\u0435 \u0445\u0440\u0430\u043d\u0438\u0442 \u043f\u0430\u043c\u044f\u0442\u044c \u043e \u0432\u0435\u043b\u0438\u043a\u043e\u043c \u0440\u0443\u0441\u0441\u043a\u043e\u043c \u043f\u043e\u044d\u0442\u0435 \u041d. \u0410. \u041d\u0435\u043a\u0440\u0430\u0441\u043e\u0432\u0435.\n\u041f\u0440\u043e\u0434\u043e\u043b\u0436\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u044c \u044d\u043a\u0441\u043a\u0443\u0440\u0441\u0438\u0438 3 \u0447\u0430\u0441\u0430.\n\n\n\u00a0\n\u0414\u043e\u043f\u043e\u043b\u043d\u0438\u0442\u0435\u043b\u044c\u043d\u0430\u044f \u044d\u043a\u0441\u043a\u0443\u0440\u0441\u0438\u044f\n\n\n\u0410\u0432\u0442\u043e\u0431\u0443\u0441\u043d\u0430\u044f \u044d\u043a\u0441\u043a\u0443\u0440\u0441\u0438\u044f \u0432\u00a0 \u043c\u0435\u043c\u043e\u0440\u0438\u0430\u043b\u044c\u043d\u044b\u0439 \u0414\u043e\u043c-\u043c\u0443\u0437\u0435\u0439 \u041b.\u0412. \u0421\u043e\u0431\u0438\u043d\u043e\u0432\u0430.\n\u041c\u0443\u0437\u0435\u0439 \u0440\u0430\u0441\u043f\u043e\u043b\u043e\u0436\u0435\u043d \u0432 \u043d\u0435\u0431\u043e\u043b\u044c\u0448\u043e\u043c \u0443\u044e\u0442\u043d\u043e\u043c \u043e\u0441\u043e\u0431\u043d\u044f\u043a\u0435, \u043f\u043e\u0441\u0442\u0440\u043e\u0435\u043d\u043d\u043e\u043c \u0432 XVIII \u0432\u0435\u043a\u0435. \u0417\u0434\u0435\u0441\u044c \u0432 1872 \u0433\u043e\u0434\u0443 \u0440\u043e\u0434\u0438\u043b\u0441\u044f \u041b\u0435\u043e\u043d\u0438\u0434 \u0412\u0438\u0442\u0430\u043b\u044c\u0435\u0432\u0438\u0447 \u0421\u043e\u0431\u0438\u043d\u043e\u0432. \u0414\u043e\u043c \u0445\u0440\u0430\u043d\u0438\u0442 \u043f\u0430\u043c\u044f\u0442\u044c \u043e \u0442\u0440\u0435\u0445 \u043f\u043e\u043a\u043e\u043b\u0435\u043d\u0438\u044f\u0445 \u0441\u0435\u043c\u044c\u0438 \u0421\u043e\u0431\u0438\u043d\u043e\u0432\u0430. \u041e\u043d \u043d\u0430\u043f\u043e\u043b\u043d\u0435\u043d \u0441\u0446\u0435\u043d\u0438\u0447\u0435\u0441\u043a\u0438\u043c\u0438 \u0438 \u0441\u0435\u043c\u0435\u0439\u043d\u044b\u043c\u0438 \u0440\u0435\u043b\u0438\u043a\u0432\u0438\u044f\u043c\u0438 \u2013 \u0440\u0435\u0434\u043a\u0438\u043c\u0438 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u0430\u043c\u0438 \u0438 \u0444\u043e\u0442\u043e\u0433\u0440\u0430\u0444\u0438\u044f\u043c\u0438 \u043e\u043f\u0435\u0440\u043d\u044b\u0445 \u0433\u0435\u0440\u043e\u0435\u0432, \u0443\u043d\u0438\u043a\u0430\u043b\u044c\u043d\u044b\u043c\u0438 \u0441\u0446\u0435\u043d\u0438\u0447\u0435\u0441\u043a\u0438\u043c\u0438 \u043a\u043e\u0441\u0442\u044e\u043c\u0430\u043c\u0438 \u0421\u043e\u0431\u0438\u043d\u043e\u0432\u0430, \u0442\u0435\u0430\u0442\u0440\u0430\u043b\u044c\u043d\u044b\u043c\u0438 \u0430\u043a\u0441\u0435\u0441\u0441\u0443\u0430\u0440\u0430\u043c\u0438, \u0440\u0435\u043a\u0432\u0438\u0437\u0438\u0442\u0430\u043c\u0438- \u0441\u0432\u0438\u0434\u0435\u0442\u0435\u043b\u044f\u043c\u0438 \u0442\u0440\u0438\u0443\u043c\u0444\u0430 \u0430\u0440\u0442\u0438\u0441\u0442\u0430, \u0442\u0440\u043e\u0433\u0430\u0442\u0435\u043b\u044c\u043d\